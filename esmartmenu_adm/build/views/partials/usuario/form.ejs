<div class="box-body">
	<div class="form-group">
		<label for="email">Email</label>
		<input type="email" name="email" class="form-control" placeholder="Insira seu e-mail" value="<%= data.email %>">
	</div>
	<div class="form-group">
		<label for="senha">Senha</label>
		<input type="password" name="senha" class="form-control" placeholder="Coloque sua senha" value="<%= data.senha %>">
	</div>
</div>

<div class="box-footer">
	<button type="submit" class="btn btn-primary">Salvar</button>
	<a href="/usuario"   class="btn btn-default">Voltar</a>
</div>
